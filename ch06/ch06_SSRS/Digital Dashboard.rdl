  <DataSets>
    <DataSet Name="PickupsAndLost">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GaugeDate">
            <Value>=Parameters!GaugeDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT COUNT(d.DeliveryNumber) AS NumOfPickups, COUNT(lp.LostPackageNumber) AS NumLost
FROM Delivery d
	LEFT OUTER JOIN LostPackage lp
		ON d.DeliveryNumber = lp.DeliveryNumber
WHERE PickupDateTime BETWEEN DATEADD(d, -28, @GaugeDate) AND @GaugeDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NumOfPickups">
          <DataField>NumOfPickups</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NumLost">
          <DataField>NumLost</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
