  <DataSets>
    <DataSet Name="FuelPrices">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Description AS FuelType, PriceStartDate, Price
FROM FuelPrice fp
	INNER JOIN Propulsion p
		ON fp.PropulsionID = p.PropulsionID
WHERE YEAR(PriceStartDate) = @Year
ORDER BY FuelType, PriceStartDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FuelType">
          <DataField>FuelType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PriceStartDate">
          <DataField>PriceStartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Years">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT DISTINCT YEAR(PriceStartDate) AS Year
FROM FuelPrice 
ORDER BY YEAR(PriceStartDate)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
